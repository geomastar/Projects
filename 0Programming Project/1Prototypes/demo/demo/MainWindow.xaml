<Window x:Name="Window" x:Class="demo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:demo"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="808" MaxHeight="500" MaxWidth="808" MinHeight="500" MinWidth="808">
    <Canvas x:Name="gameCanvas">
        <Path Name="circleboi" Stroke="Black" StrokeThickness="1" Canvas.Left="113" Canvas.Top="128">
            <Path.Data>
                <EllipseGeometry RadiusX="50" RadiusY="50"/>
            </Path.Data>
        </Path>
        <Path Name="squareboi" Stroke="Black" StrokeThickness="1" Canvas.Left="143" Canvas.Top="128">
            <Path.Data>
                <RectangleGeometry Rect="10,10,10,10"/>
            </Path.Data>
        </Path>
    </Canvas>
</Window>
